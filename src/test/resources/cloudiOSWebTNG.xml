<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="EPAM mobile cloud Web testing Suite">
    <parameter name="appType" value="web"></parameter>
<!--    <parameter name="deviceName" value="IPHONE XR"></parameter>-->
    <parameter name="udid" value="00008030-001525D91A92802E"></parameter>
    <parameter name="platformName" value="iOS"></parameter>
    <parameter name="browserName" value="Safari"></parameter>
    <test name="Cloud Web tests">
        <groups>
            <run>
                <include name="cloud"/>
            </run>
        </groups>
        <packages>
            <package name="scenarios"/>
        </packages>
    </test>
</suite>
